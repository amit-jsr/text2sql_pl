# LLM Configuration
# Supported backends: openai, groq, ollama, bedrock
LLM_BACKEND=openai

# OpenAI (recommended for best SQL generation)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
# Alternative models: gpt-4o-mini (cheaper), gpt-4-turbo

# Groq (free tier available - good alternative)
# LLM_BACKEND=groq
# GROQ_API_KEY=your_groq_api_key_here
# GROQ_BASE_URL=https://api.groq.com/openai/v1
# GROQ_MODEL=llama-3.3-70b-versatile

RAG_BACKEND=local

# Chainlit (optional)
LITERAL_API_KEY=
